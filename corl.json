{
  "builders": {
    "package": {
      "environment": {
        "production": {
          "coralnexus__core": "github:::coralnexus/corl-package-core[923807f763d5c01421d3dec401f1c594b9464717]"
        },
        "qa": {
          "use": "production"
        },
        "default": {
          "coralnexus__core": "github:::coralnexus/corl-package-core"
        }
      }
    }
  },
  "provisioners": {
    "puppetnode": {
      "coralnexus::portal": {
        "profiles": {
          "apache_server": {
            "environment": {
              "production": {
                "modules": {
                  "apache": "github:::coralnexus/puppet-apache[8a1d8d0d81386765e59265c5cd2b04b37a892838]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "apache": "github:::coralnexus/puppet-apache"
                }
              }
            }
          },
          "php": {
            "environment": {
              "production": {
                "modules": {
                  "php": "github:::coralnexus/puppet-php[1875d16775ffcebcab56ccd74f0806d1d05116a2]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "php": "github:::coralnexus/puppet-php"
                }
              }
            }
          },
          "apache_static": {
            "extend": "apache_server"
          },
          "apache_drupal": {
            "extend": [ "apache_server", "php" ],
            "environment": {
              "production": {
                "modules": {
                  "drupal": "github:::coralnexus/puppet-drupal[c1c0ec35e40f11fd4a75ad7841846549b5eb8e5c]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "drupal": "github:::coralnexus/puppet-drupal"
                }
              }
            }
          },
          "percona_server": {
            "environment": {
              "production": {
                "modules": {
                  "percona": "github:::coralnexus/puppet-percona[9463414a03327ffe40e2de6f7a2d7fc406fa0f47]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "percona": "github:::coralnexus/puppet-percona"
                }
              }
            }
          },
          "varnish_server": {
            "environment": {
              "production": {
                "modules": {
                  "varnish": "github:::maxchk/puppet-varnish[b9846d1b35e87a45c98213fddfe71fa0a6f3b31c]",
                  "varnish_drupal": "github:::coralnexus/puppet-varnish_drupal[b1f8ecd9be5144d642d7717429a3652234b5f0d9]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "varnish": "github:::maxchk/puppet-varnish[develop]",
                  "varnish_drupal": "github:::coralnexus/puppet-varnish_drupal"
                }
              }
            }
          }
        }
      }
    }
  }
}
